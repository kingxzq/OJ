class Solution {
public:
    int rob(vector<int>& nums) {
        int n=nums.size();
        vector<int> dp1(n);
        vector<int> dp2(n);
        dp1[0]=nums[0];

        for(int i=1;i<n;i++){
            dp1[i]+=dp2[i-1]+nums[i];
            dp2[i]=max(dp2[i-1],dp1[i-1]);
        }

        return max(dp1[n-1],dp2[n-1]);
    }
};